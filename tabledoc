CREATE TABLE
  main (
    MAIN_ID INT (11) PRIMARY KEY AUTO_INCREMENT,
    MAIN_TITLE VARCHAR(255),
    MAIN_DESC VARCHAR(255),
    DEL_FRAG VARCHAR(1) DEFAULT 'N',
    CREATE_DATE DATETIME DEFAULT CURRENT_TIMESTAMP,
    UPDATE_DATE DATETIME DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP
  );

CREATE TABLE
  img (
    IMG_ID INT (11) PRIMARY KEY AUTO_INCREMENT,
    IMG_IMG LONGBLOB,
    MAIN_ID INT (11),
    DEL_FRAG VARCHAR(1) DEFAULT 'N',
    CREATE_DATE DATETIME DEFAULT CURRENT_TIMESTAMP,
    UPDATE_DATE DATETIME DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP
  );

CREATE TABLE
  sub (
    SUB_ID INT(11) PRIMARY KEY AUTO_INCREMENT,
    SUB_TITLE VARCHAR(255),
    SUB_DETAIL VARCHAR(255),
    YOUTUBE VARCHAR(255),
    STEAM VARCHAR(255),
    MAIN_ID INT(11),
    DEL_FRAG VARCHAR(1) DEFAULT 'N',
    CREATE_DATE DATETIME DEFAULT CURRENT_TIMESTAMP,
    UPDATE_DATE DATETIME DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP
  );
